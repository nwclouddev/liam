= render 'shared/errors', obj: @event_type
= form_for @event_type, html: { class: "form-horizontal", role: "form" } do |f|
  .form-group
    = f.label :seq
    = f.number_field :seq, class: 'form-control'
  .form-group
    = f.label :description
    = f.text_field :description, class: 'form-control'
  .form-group.form-check
    = f.check_box :user_createable, class: 'form-check-input'
    = f.label :user_createable, "User Creatable", class: 'form-check-label'  
  .form-group.form-check
    = f.check_box :not_used, class: 'form-check-input'
    = f.label :not_used, "Out of Service", class: 'form-check-label'
  = render 'shared/form_navigation'